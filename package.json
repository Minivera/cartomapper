{
  "name": "cartomapper",
  "version": "1.0.0",
  "description": "Create maps using React and Typescript",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsdx build",
    "start": "parcel example/index.html --open",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "prepublishOnly": "npm run build",
    "test": "tsdx test --env=jsdom",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "clean": "git clean -fdX ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minivera/cartomapper.git"
  },
  "keywords": [
    "TypeScript",
    "React",
    "Mapping"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Minivera/cartomapper/issues"
  },
  "homepage": "https://github.com/Minivera/cartomapper#readme",
  "main": "dist/index.js",
  "umd:main": "dist/react-vitae.umd.production.js",
  "module": "dist/react-vitae.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "author": "Guillaume St-Pierre",
  "resolutions": {
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-jest": "^24.0.2",
    "tsdx": "0.7.2",
    "typescript": "^3.4.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each.js"
    ]
  }
}
